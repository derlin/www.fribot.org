--

CREATE TABLE `wp_wpb2d_premium_extensions` (
  `name` varchar(50) collate latin1_german1_ci NOT NULL,
  `file` varchar(255) collate latin1_german1_ci NOT NULL,
  UNIQUE KEY `name` (`name`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 COLLATE=latin1_german1_ci;

--
-- Table `wp_wpb2d_premium_extensions` is empty
--

--
-- Table structure for table `wp_wpb2d_processed_dbtables`
--

CREATE TABLE `wp_wpb2d_processed_dbtables` (
  `name` varchar(255) collate latin1_german1_ci NOT NULL,
  `count` int(11) NOT NULL default '0',
  UNIQUE KEY `name` (`name`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 COLLATE=latin1_german1_ci;

