--
-- Table structure for table `wp_fm_forms`
--

CREATE TABLE `wp_fm_forms` (
  `ID` int(11) NOT NULL default '0',
  `title` text NOT NULL,
  `submitted_msg` text NOT NULL,
  `submit_btn_text` varchar(32) NOT NULL default '',
  `required_msg` text NOT NULL,
  `data_table` varchar(32) NOT NULL default '',
  `action` text NOT NULL,
  `data_index` varchar(32) NOT NULL default '',
  `shortcode` varchar(64) NOT NULL default '',
  `type` varchar(32) NOT NULL default '',
  `email_list` text NOT NULL,
  `behaviors` varchar(256) NOT NULL default '',
  `email_user_field` varchar(64) NOT NULL default '',
  `form_template` varchar(128) NOT NULL default '',
  `email_template` varchar(128) NOT NULL default '',
  `email_subject` varchar(1024) NOT NULL default '',
  `email_from` varchar(1024) NOT NULL default '',
  `summary_template` varchar(128) NOT NULL default '',
  `template_values` text NOT NULL,
  `show_summary` tinyint(1) NOT NULL default '0',
  `use_advanced_email` tinyint(1) NOT NULL default '0',
  `advanced_email` text NOT NULL,
  `publish_post` tinyint(1) NOT NULL default '0',
  `publish_post_category` text NOT NULL,
  `publish_post_title` text NOT NULL,
  `publish_post_status` varchar(16) NOT NULL default 'publish',
  `auto_redirect` tinyint(1) NOT NULL default '0',
  `auto_redirect_page` int(11) NOT NULL default '0',
  `auto_redirect_timeout` int(11) NOT NULL default '5',
  `conditions` text NOT NULL,
  `reg_user_only_msg` text NOT NULL,
  `summary_hide_empty` tinyint(1) NOT NULL default '0',
  `exact_form_action` varchar(1024) NOT NULL default '',
  `enable_autocomplete` tinyint(1) NOT NULL default '1',
  PRIMARY KEY  (`ID`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;

--
-- Dumping data for table `wp_fm_forms`
--

