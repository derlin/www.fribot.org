-- Table structure for table `wp_fm_settings`
--

CREATE TABLE `wp_fm_settings` (
  `setting_name` varchar(32) NOT NULL,
  `setting_value` text NOT NULL,
  PRIMARY KEY  (`setting_name`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;

