--
-- Table structure for table `wp_simple_login_log`
--

CREATE TABLE `wp_simple_login_log` (
  `id` int(11) NOT NULL auto_increment,
  `uid` int(11) NOT NULL,
  `user_login` varchar(60) collate latin1_german1_ci NOT NULL,
  `user_role` varchar(30) collate latin1_german1_ci NOT NULL,
  `time` datetime NOT NULL default '0000-00-00 00:00:00',
  `ip` varchar(100) collate latin1_german1_ci NOT NULL,
  `login_result` varchar(1) collate latin1_german1_ci default NULL,
  `data` longtext collate latin1_german1_ci NOT NULL,
  PRIMARY KEY  (`id`),
  KEY `uid` (`uid`,`ip`,`login_result`)
) ENGINE=MyISAM AUTO_INCREMENT=55 DEFAULT CHARSET=latin1 COLLATE=latin1_german1_ci;

