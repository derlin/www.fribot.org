--
-- Table structure for table `wp_fm_items`
--

CREATE TABLE `wp_fm_items` (
  `ID` int(11) NOT NULL default '0',
  `index` int(11) NOT NULL default '0',
  `unique_name` varchar(64) NOT NULL default '',
  `type` varchar(32) NOT NULL default '',
  `extra` text NOT NULL,
  `nickname` text NOT NULL,
  `label` text NOT NULL,
  `required` tinyint(1) NOT NULL default '0',
  `db_type` varchar(16) NOT NULL default '',
  `description` text NOT NULL,
  `set` int(11) NOT NULL default '0',
  UNIQUE KEY `unique_name` (`unique_name`),
  UNIQUE KEY `unique_name_2` (`unique_name`),
  UNIQUE KEY `unique_name_3` (`unique_name`),
  UNIQUE KEY `unique_name_4` (`unique_name`),
  UNIQUE KEY `unique_name_5` (`unique_name`),
  KEY `ID` (`ID`),
  KEY `ID_2` (`ID`),
  KEY `ID_3` (`ID`),
  KEY `ID_4` (`ID`),
  KEY `ID_5` (`ID`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;

