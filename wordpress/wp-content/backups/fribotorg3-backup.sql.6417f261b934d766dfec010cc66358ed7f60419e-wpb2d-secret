CREATE TABLE `wp_fm_templates` (
  `title` text NOT NULL,
  `filename` text NOT NULL,
  `content` text NOT NULL,
  `status` varchar(32) NOT NULL default '',
  `modified` bigint(20) NOT NULL
) ENGINE=MyISAM DEFAULT CHARSET=utf8;

